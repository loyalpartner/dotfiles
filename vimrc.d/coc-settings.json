{
  "suggest.noselect": false,
  "suggest.completionItemKindLabels": {
    "text": "text",
    "method": "method",
    "function": "function",
    "constructor": "constructor",
    "field": "field",
    "variable": "variable",
    "class": "class",
    "interface": "interface",
    "module": "module",
    "property": "property",
    "unit": "unit",
    "value": "value",
    "enum": "enum",
    "keyword": "keyword",
    "snippet": "snippet",
    "color": "color",
    "file": "file",
    "reference": "reference",
    "folder": "folder",
    "enumMember": "enumMember",
    "constant": "constant",
    "struct": "struct",
    "event": "event",
    "operator": "operator",
    "typeParameter": "typeParameter",
    "default": "default"
  },
  "inlayHint.enable": true,
  "suggest.floatConfig": {
    "shadow": true,
    "border": true,
    "rounded": true,
    "borderhighlight": "CocFloating",
    "highlight": "CocFloating"
  },
  // "suggest.languageSourcePriority": 99,
  "suggest.virtualText": true,
  "outline.followCursor": true,
  "colors.enable": true,
  "json.schemas": [
    {
      "fileMatch": [
        ".vimspector.json"
      ],
      "url": "https://puremourning.github.io/vimspector/schema/vimspector.schema.json"
    }
  ],
  "workspace.rootPatterns": [
    ".git",
    ".hg",
    ".projections.json"
  ],
  "coc.preferences.floatActions": false,
  "list.source.files.command": "rg",
  // "list.menuAction": true,
  "list.source.files.args": [
    "--files",
    "-workspace"
  ],
  "list.source.files.excludePatterns": [
    "**/node_modules/**"
  ],
  "translator.engines": [
    "youdao",
    "bing",
    "haici"
  ],
  "git.signPriority": 1,
  "html.format.enable": true,
  "html.validate.scripts": true,
  "html.autoClosingTags": true,
  "tsserver.disableAutomaticTypeAcquisition": true,
  "tsserver.trace.server": "verbose",
  "pyright.inlayHints.variableTypes": false,
  "pyright.inlayHints.functionReturnTypes": true,
  "snippets.userSnippetsDirectory": "~/dotfiles/snippets/",
  "snippets.priority": 100,
//   "gn.execArgv": [
//     "--nolazy",
//     "--inspect=6047"
//   ],
  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": [
        "elm"
      ],
      "rootPatterns": [
        ".git/",
        "elm.json"
      ],
      "initializationOptions": {
        "elmPath": "elm", // optional
        "elmFormatPath": "elm-format", // optional
        "elmTestPath": "elm-test", // optional
        "elmAnalyseTrigger": "change" // optional
      }
    },
    // cargo install mojom-lsp
    "mojom": {
      "command": "mojom-lsp",
      "rootPatterns": [
        ".git/"
      ],
      "filetypes": [
        "mojom"
      ]
    },
    // https://github.com/lasorda/protobuf-language-server.git
    // need bead
    "proto": {
      "command": "pls",
      "rootPatterns": [
        ".git/"
      ],
      "filetypes": [
        "proto"
      ]
    },
    "jsonnet": {
      "command": "jsonnet-language-server",
      "args": [
        "-t"
      ],
      "rootPatterns": [
        ".git/",
        "jsonnetfile.json"
      ],
      "filetypes": [
        "jsonnet",
        "libsonnet"
      ]
    }
  },
  "snippets.ultisnips.pythonPrompt": false,
  "git.enableGutters": true
}
