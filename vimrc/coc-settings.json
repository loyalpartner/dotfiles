{
  "suggest.noselect": false,
  "suggest.selection": "first",
  "suggest.completionItemKindLabels": {
    "text": "text",
    "method": "method",
    "function": "function",
    "constructor": "constructor",
    "field": "field",
    "variable": "variable",
    "class": "class",
    "interface": "interface",
    "module": "module",
    "property": "property",
    "unit": "unit",
    "value": "value",
    "enum": "enum",
    "keyword": "keyword",
    "snippet": "snippet",
    "color": "color",
    "file": "file",
    "reference": "reference",
    "folder": "folder",
    "enumMember": "enumMember",
    "constant": "constant",
    "struct": "struct",
    "event": "event",
    "operator": "operator",
    "typeParameter": "typeParameter",
    "default": "default"
  },
  "suggest.floatConfig": {
    "shadow": true,
    "border": true,
    "rounded": true,
    "borderhighlight": "CocFloating",
    "highlight": "CocFloating"
  },
  "suggest.languageSourcePriority": 1000000,
  "suggest.virtualText": true,
  "outline.followCursor": true,
  "coc.preferences.rootPatterns": [
    ".git",
    ".hg",
    ".projections.json"
  ],
  "coc.preferences.floatActions": false,
  "list.source.files.command": "rg",
  "list.source.files.args": [
    "--files"
  ],
  "list.source.files.defaultArgs": [
    "-F"
  ],
  "list.source.files.excludePatterns": [
    "**/node_modules/**"
  ],
  "translator.engines": [
    "youdao",
    "bing",
    "haici"
  ],
  "git.signPriority": 1,
  "html.format.enable": true,
  "html.validate.scripts": true,
  "html.autoClosingTags": true,
  "tsserver.disableAutomaticTypeAcquisition": true,
  "pyright.inlayHints.variableTypes": false,
  "pyright.inlayHints.functionReturnTypes": true,
  "snippets.userSnippetsDirectory": "~/dotfiles/vimrc/snippets/",
  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": [
        "elm"
      ],
      "rootPatterns": [
        "elm.json"
      ],
      "initializationOptions": {
        "elmPath": "elm", // optional
        "elmFormatPath": "elm-format", // optional
        "elmTestPath": "elm-test", // optional
        "elmAnalyseTrigger": "change" // optional
      }
    },
    "jsonnet": {
      "command": "jsonnet-language-server",
      "args": [
        "-t"
      ],
      "rootPatterns": [
        ".git/",
        "jsonnetfile.json"
      ],
      "filetypes": [
        "jsonnet",
        "libsonnet"
      ]
    }
  }
}
